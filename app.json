{
  "expo": {
    "name": "L&D Guest Marbella",
    "slug": "ld-guest-marbella",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "ldguest",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["web"],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "L&D Guest Marbella",
      "shortName": "L&D Guest",
      "lang": "en",
      "scope": "/",
      "themeColor": "#E9B872",
      "backgroundColor": "#FFFFFF",
      "display": "standalone",
      "startUrl": "/",
      "orientation": "any",
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons",
            "expo-router",
            "react-native-web"
          ]
        }
      }
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}